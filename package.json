  "bin": {
    "lightning-jackpot": "./bin/lightning-jackpot"
  },
  "workspaces": [
    "./src/packages/*"
  ],
  "scripts": {
    "lerna": "lerna",
    "cache:clear": "typeorm cache:clear",
    "lint": "yarn lint:backend && yarn lint:frontend",
    "lint:backend": "eslint --fix --ext .js,.ts src/backend/",
    "lint:frontend": "eslint --fix --ext .js,.tsx,.ts src/frontend/",
    "seed": "./bin/lightning-jackpot seed",
    "seed:run": "./bin/lightning-jackpot seed --run",
    "start": "pm2-runtime --raw pm2.yml",
    "start:dev2": "concurrently \"yarn start:watch\" \"pm2-dev --raw pm2-dev.yml\"",
    "start:dev": "tsc-watch -p src/backend/tsconfig.json --onFirstSuccess \"pm2-dev --raw pm2-dev.yml\" --onSuccess \"echo Beep! Compilation Successful\" --onFailure \"echo Beep! Compilation Failed\"",
    "build-packages": "yarn build-common && yarn build-wheel && yarn build-testing",
    "build-common": "cd src/packages/common && yarn build",
    "build-wheel": "cd src/packages/react-wheel && yarn build",
    "build-testing": "cd src/packages/testing && yarn build",
    "build": "cross-env NODE_ENV=production yarn build-packages && ./node_modules/.bin/next build src/frontend && ./node_modules/.bin/tsc --project src/backend/tsconfig.json",
    "clean": "rimraf dist && rimraf .next",
    "debug": "nodemon --inspect src/backend/index.ts",
    "debug2": "node --inspect-brk ts-node -r tsconfig-paths/register -m commonjs --transpile-only src/backend/index.ts",
    "dev": "pm2-dev start pm2-dev.yml",
    "test": "cross-env NODE_ENV=test TYPEORM_LOGGING=false yarn run test:frontend && yarn run test:backend",
    "diagnose": "node --max-old-space-size=8192 tsc --diagnostics --project src/frontend/tsconfig.json",
    "test:backend": "jest -c src/backend/jest.config.js --detectOpenHandles --forceExit --runInBand",
    "test:frontend": "jest -c src/frontend/jest.config.js --passWithNoTests",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:integration": "cross-env NODE_ENV=test jest -c tests/integration/jest.config.js",
    "test:mem": "npm test -- --coverage --silent --logHeapUsage",
    "test:prepublish": "yarn lint && yarn test -- --silent --no-cache",
    "test:prod": "yarn test -- --coverage --no-cache",
    "test:watch": "jest --watch",
    "watch": "tsc -w -p src/backend/tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts*": [
      "yarn lint:fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
