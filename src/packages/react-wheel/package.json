{
  "name": "react-wheel",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "unpkg": "dist/index.umd.js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "yarn run clean && microbundle src/index.tsx -f modern,umd --globals react-spring=ReactSpring",
    "build2": "yarn run clean && microbundle build --tsconfig tsconfig.json --target node --globals react-spring=ReactSpring --sourcemap false",
    "dev": "microbundle watch",
    "lint": "eslint --ext .tsx,.ts src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "react": ">= 16.9.0",
    "react-spring": ">= 8.0 "
  },
  "dependencies": {
    "react-spring": "^8.0.27"
  },
  "devDependencies": {
    "@rehooks/component-size": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "classnames": "^2.2.0",
    "eslint": "^6.8.0",
    "microbundle": "next",
    "typescript": "^3.7.0"
  }
}
